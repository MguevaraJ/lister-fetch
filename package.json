{
  "name": "lister",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=\"production\" babel-node src/index.js",
    "postinstall": "npm run build",
    "build": "cross-env NODE_ENV=\"production\" concurrently \"rimraf src/public\" \"webpack\"",
    "watch:node": "cross-env NODE_ENV=\"development\" nodemon src/index.js",
    "watch:webpack": "cross-env NODE_ENV=\"development\" webpack -w",
    "start:dev": "concurrently \"npm run watch:node\" \"npm run watch:webpack\""
  },
  "keywords": [],
  "author": "Mois√©s Guevara",
  "license": "GPL",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "colors": "^1.3.3",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^6.7.3",
    "dotenv": "^6.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.3",
    "style-loader": "^3.3.1",
    "timeago.js": "^4.0.0-beta.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MguevaraJ/lister-fetch"
  },
  "dependencies": {
    "express": "^4.16.4",
    "mysql": "^2.16.0"
  },
  "nodemonConfig": {
    "ignore": [
      "src/app/*"
    ],
    "verbose": false,
    "execMap": {
      "js": "babel-node"
    }
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  }
}
