{
  "name": "lister",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=\"production\" babel-node src/index.js",
    "prod:webpack": "cross-env NODE_ENV=\"production\" webpack",
    "start:prod": "concurrently \"npm start\" \"npm run prod:webpack\"",
    "watch:node": "cross-env NODE_ENV=\"development\" nodemon src/index.js",
    "watch:webpack": "cross-env NODE_ENV=\"development\" webpack -w",
    "watch:dev": "concurrently \"npm run watch:node\" \"npm run watch:webpack\""
  },
  "keywords": [],
  "author": "Mois√©s Guevara",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "colors": "^1.3.3",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.15.1",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "style-loader": "^0.23.1",
    "timeago.js": "^4.0.0-beta.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MguevaraJ/lister-fetch-practice"
  },
  "dependencies": {
    "express": "^4.16.4",
    "mysql": "^2.16.0"
  },
  "nodemonConfig": {
    "ignore": [
      "front-end/*"
    ],
    "verbose": false,
    "execMap": {
      "js": "babel-node"
    }
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 10
    },
    "rules": {
      "quotes": ["error","single"]
    },
    "extends": "semistandard"
  }
}